{
  "name": "genkitx-astra-db",
  "description": "An Astra DB indexer and retriever for Genkit",
  "version": "0.2.0",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "check": "tsc",
    "build:compile": "tsup ./src",
    "build:clean": "rm -rf ./dist",
    "build:watch": "tsup ./src --watch",
    "build": "npm run build:clean && npm run build:compile",
    "lint": "eslint .",
    "test": "npm run check && npm run lint"
  },
  "contributors": [
    "Phil Nash <phil.nash@datastax.com> (https://philna.sh)"
  ],
  "license": "Apache-2.0",
  "keywords": [
    "genkit",
    "genkit-plugin",
    "genkit-retriever",
    "genkit-indexer",
    "ai",
    "genai",
    "generative-ai",
    "vector",
    "embedding",
    "astra-db",
    "datastax"
  ],
  "dependencies": {
    "@datastax/astra-db-ts": "^1.5.0",
    "ts-md5": "^1.3.1"
  },
  "peerDependencies": {
    "genkit": "^1.0.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.1",
    "@types/eslint__js": "^8.42.3",
    "eslint": "^9.9.1",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.2.0"
  }
}
